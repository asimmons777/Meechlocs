{
  "name": "meechlocs-api",
  "version": "0.1.0",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc -p .",
    "start": "node ./scripts/bootstrap.js",
    "start:prod": "prisma generate --schema=./prisma/schema.prisma && node ./scripts/bootstrap.js",
    "prisma:generate": "prisma generate --schema=./prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema=./prisma/schema.prisma --name init",
    "prisma:studio": "prisma studio --schema=./prisma/schema.prisma",
    "seed": "node ./prisma/seed.js",
    "seed-demo": "node ./scripts/seed-demo.js",
    "seed-demo-user": "node ./scripts/seed-demo-user.js",
    "purge-demo": "node ./scripts/purge-demo.js"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "@sendgrid/mail": "^7.7.0",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "prisma": "^5.22.0",
    "stripe": "^12.0.0",
    "multer": "^1.4.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.5.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
  }
}
